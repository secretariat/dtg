layout 'admin'
<%= link_to "Новый Админ", :controller => 'admin', :action => 'new' %><br/>
<br/>
<br/>
<br/>
<table>
	<th>N%</th>
	<th>Email:</th>
	<th>Login:</th>
	<th>Pass:</th>
	<th>Created:</th>
	<% @admin.each do |ad| %>	
		<tr>
			<td><%= ad.id %></td>
			<td><%= ad.email %></td>
			<td><%= ad.main.login %></td>
			<td><%= ad.main.hash_pass %></td>
			<td><%= ad.main.created_at %></td>

		</tr>
	<% end %>
</table>
<br/>